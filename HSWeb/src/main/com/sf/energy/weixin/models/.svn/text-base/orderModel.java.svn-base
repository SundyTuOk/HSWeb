package com.sf.energy.weixin.models;

public class orderModel
{
	private int Id;
	private int ammeterId;
	private String order_Money;
	private String order_Date;
	private String order_No;
	private int order_Flag;
	private String saleInfoNum;
	public int getId()
	{
		return Id;
	}
	public void setId(int id)
	{
		Id = id;
	}
	public int getAmmeterId()
	{
		return ammeterId;
	}
	public void setAmmeterId(int ammeterId)
	{
		this.ammeterId = ammeterId;
	}
	public String getOrder_Money()
	{
		return order_Money;
	}
	public void setOrder_Money(String order_Money)
	{
		this.order_Money = order_Money;
	}
	public String getOrder_Date()
	{
		return order_Date;
	}
	public void setOrder_Date(String order_Date)
	{
		this.order_Date = order_Date;
	}
	public String getOrder_No()
	{
		return order_No;
	}
	public void setOrder_No(String order_No)
	{
		this.order_No = order_No;
	}
	public int getOrder_Flag()
	{
		return order_Flag;
	}
	public void setOrder_Flag(int order_Flag)
	{
		this.order_Flag = order_Flag;
	}
	public String getSaleInfoNum()
	{
		return saleInfoNum;
	}
	public void setSaleInfoNum(String saleInfoNum)
	{
		this.saleInfoNum = saleInfoNum;
	}

}
