package com.sf.energy.project.system.model;

/**
 * <建筑物实体类，并提供了set和get方法>
 * 
 * @author 李涵淼
 * @version 1.0
 * @since[version 1.0]
 * 
 */

public class Architecture
{

	// / id为建筑的ID
	private int id = 0;

	// / areaID为建筑所属区域的ID
	private int areaID = 0;

	// / Num为建筑的编号
	private String Num = "";

	// / Name为建筑的名称
	private String Name = "";

	// / style为建筑的类别
	private String Style = "";

	// / Time为建筑的建造时间
	private String Time = "";

	// / storey为建筑的总楼层
	private int Storey = 1;

	// / Area为建筑的建筑面积
	private float Area = 0;

	// / Aircondition为建筑的空调建筑面积
	private float Aircondition = 0;

	// / Function为建筑的ID
	private String Function = "";

	// / FL为建筑的分类
	private String FL = "";

	// / Floorstart为建筑的起始楼层
	private int Floorstart = 1;

	// / imgUrl为图片的存储路径
	private String imgUrl = "";

	// / Man为建筑的负责人
	private String Man = "";

	// / Payment说明建筑是否预付费
	private int Payment = 0;

	// / Phone为建筑的联系电话
	private String Phone = "";

	// / Count_man,常驻人数
	int countMan = 0;

	private String archStyleName;
	
	private int isUnit= 0;
	
	public Architecture(int id, int areaID, String architectureNum,
			String architectureName, String architectureStyle,
			String architectureTime, int architectureStorey,
			float architectureArea, float architectureAircondition,
			String architectureFunction, String architectureFL,
			int architectureFloorstart, String architectureimgUrl,
			String architectureMan, int architecturePayment,
			String architecturePhone,int isUnit)
	{

		this.id = id;
		this.areaID = areaID;
		this.Num = architectureNum;
		this.Name = architectureName;
		this.Style = architectureStyle;
		this.Time = architectureTime;
		this.Storey = architectureStorey;
		this.Area = architectureArea;
		this.Aircondition = architectureAircondition;
		this.Function = architectureFunction;
		this.FL = architectureFL;
		this.Floorstart = architectureFloorstart;
		this.imgUrl = architectureimgUrl;
		this.Man = architectureMan;
		this.Payment = architecturePayment;
		this.Phone = architecturePhone;
		this.setIsUnit(isUnit);
	}

	public Architecture()
	{

	}

	/**
	 * 
	 * 建筑各个属性的set和get方法
	 */

	public int getId()
	{
		return id;
	}

	public String getArchStyleName()
	{
		return archStyleName;
	}

	public void setArchStyleName(String archStyleName)
	{
		this.archStyleName = archStyleName;
	}

	public void setId(int id)
	{
		this.id = id;
	}

	public int getAreaID()
	{
		return areaID;
	}

	public void setAreaID(int areaID)
	{
		this.areaID = areaID;
	}

	public String getNum()
	{
		return Num;
	}

	public void setNum(String num)
	{
		Num = num;
	}

	public String getName()
	{
		return Name;
	}

	public void setName(String name)
	{
		Name = name;
	}

	public String getStyle()
	{
		return Style;
	}

	public void setStyle(String style)
	{
		Style = style;
	}

	public String getTime()
	{
		return Time;
	}

	public void setTime(String time)
	{
		Time = time;
	}

	public int getStorey()
	{
		return Storey;
	}

	public void setStorey(int storey)
	{
		Storey = storey;
	}

	public float getArea()
	{
		return Area;
	}

	public void setArea(float area)
	{
		Area = area;
	}

	public float getAircondition()
	{
		return Aircondition;
	}

	public void setAircondition(float aircondition)
	{
		Aircondition = aircondition;
	}

	public String getFunction()
	{
		return Function;
	}

	public void setFunction(String function)
	{
		Function = function;
	}

	public String getFL()
	{
		return FL;
	}

	public void setFL(String fL)
	{
		FL = fL;
	}

	public int getFloorstart()
	{
		return Floorstart;
	}

	public void setFloorstart(int floorstart)
	{
		Floorstart = floorstart;
	}

	public String getImgUrl()
	{
		return imgUrl;
	}

	public void setImgUrl(String imgUrl)
	{
		this.imgUrl = imgUrl;
	}

	public String getMan()
	{
		return Man;
	}

	public void setMan(String man)
	{
		Man = man;
	}

	public int getPayment()
	{
		return Payment;
	}

	public void setPayment(int payment)
	{
		Payment = payment;
	}

	public String getPhone()
	{
		return Phone;
	}

	public void setPhone(String phone)
	{
		Phone = phone;
	}

	public int getCountMan()
	{
		return countMan;
	}

	public void setCountMan(int countMan)
	{
		this.countMan = countMan;
	}

	public int getIsUnit()
	{
		return isUnit;
	}

	public void setIsUnit(int isUnit)
	{
		this.isUnit = isUnit;
	}

}
