package com.sf.energy.project.system.model;

import javax.xml.bind.annotation.XmlRootElement;

/**
 * <区域实体类，并提供了set和get方法>
 * 
 * @author 李涵淼
 * @version 1.0
 * @since[version 1.0]
 * 
 */
@XmlRootElement
public class Area
{
	// / id为区域的ID
	private int id;

	// / Num为区域的编号
	private String Num;

	// / Name为区域的名称
	private String Name;

	// / Address为区域的地址
	private String Address;

	// / Phone为区域的电话
	private String Phone;

	// / remark为区域的备注
	private String Remark;

	public Area(int id, String areaNum, String areaName, String areaAddress,
			String areaPhone, String areaRemark)
	{

		this.id = id;
		this.Num = areaNum;
		this.Name = areaName;
		this.Address = areaAddress;
		this.Phone = areaPhone;
		this.Remark = areaRemark;

	}

	public Area()
	{

	}

	/**
	 * 
	 * 区域属性的get和set方法
	 */

	public int getId()
	{
		return id;
	}

	public void setId(int id)
	{
		this.id = id;
	}

	public String getNum()
	{
		return Num;
	}

	public void setNum(String num)
	{
		Num = num;
	}

	public String getName()
	{
		return Name;
	}

	public void setName(String name)
	{
		Name = name;
	}

	public String getAddress()
	{
		return Address;
	}

	public void setAddress(String address)
	{
		Address = address;
	}

	public String getPhone()
	{
		return Phone;
	}

	public void setPhone(String phone)
	{
		Phone = phone;
	}

	public String getRemark()
	{
		return Remark;
	}

	public void setRemark(String remark)
	{
		Remark = remark;
	}

}