package com.sf.energy.prepayment.model;

public class CMArchModel
{
	// /电表ID
	private int amMeterID = 0;
	// /电表名称
	private String amMeterName = "";
	// /当天用电
	private float dayGross = 0;
	// /剩余电量
	private float remainValue = 0;
	//总示数
	private String zValuezy = "";

	// /在线离线状态
	private int amMeterState = 0;
	// /颜色分类(1正常绿色；2低于警戒值橙色；3无剩余电量红色；4离线灰色)
	private int colorStyle = 0;
	// /楼层号
	private int floorNum = 0;

	
	public String getzValuezy()
	{
		return zValuezy;
	}

	public void setzValuezy(String zValuezy)
	{
		this.zValuezy = zValuezy;
	}

	public int getAmMeterID()
	{
		return amMeterID;
	}

	public void setAmMeterID(int amMeterID)
	{
		this.amMeterID = amMeterID;
	}

	public String getAmMeterName()
	{
		return amMeterName;
	}

	public void setAmMeterName(String amMeterName)
	{
		this.amMeterName = amMeterName;
	}

	public float getDayGross()
	{
		return dayGross;
	}

	public void setDayGross(float dayGross)
	{
		this.dayGross = dayGross;
	}

	public float getRemainValue()
	{
		return remainValue;
	}

	public void setRemainValue(float remainValue)
	{
		this.remainValue = remainValue;
	}

	public int getAmMeterState()
	{
		return amMeterState;
	}

	public void setAmMeterState(int amMeterState)
	{
		this.amMeterState = amMeterState;
	}

	public int getColorStyle()
	{
		return colorStyle;
	}

	public void setColorStyle(int colorStyle)
	{
		this.colorStyle = colorStyle;
	}

	public int getFloorNum()
	{
		return floorNum;
	}

	public void setFloorNum(int floorNum)
	{
		this.floorNum = floorNum;
	}

}
